model ModelName

  options noimplicit
  uses "mmxprs"
  ! uses "mminsight" ! uncomment this line for an Xpress Insight model

  declarations
    x1, x2: mpvar
  end-declarations

  2*x1 + 6*x2 <= 2100
  6*x1 + 5*x2 <= 2100
  x1 >= 80

  minimise(-4*x1 - 5*x2)
  writeln("Begin running model")
  writeln("f = ", getobjval)
  writeln("x1 = ", getsol(x1))
  writeln("x2 = ", getsol(x1))
  writeln("End running model")
end-model
